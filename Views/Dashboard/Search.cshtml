@{
}
@model SearchViewModel

<div class="content">

    <h2>Search Results</h2>

    <h3>Customers</h3>
    <ul>
        @foreach (var c in Model.Customers)
        {
            <li>@c.CustomerName — @c.Phone</li>
        }
    </ul>

    <h3>Employees</h3>
    <ul>
        @foreach (var e in Model.Employees)
        {
            <li>@e.EmployeeName — @e.Email</li>
        }
    </ul>

    <h3>Reports</h3>
    <ul>
        @* @foreach (var r in Model.Reports)
        {
            <li>@r.ReportTitle — @r.Description</li>
        } *@
    </ul>

</div>
